		function checkBoxcheck(){
			var yearsCheck = document.getElementById('years-check').checked;
			var tosCheck = document.getElementById('tos-check').checked;
			if(yearsCheck && tosCheck){
				document.getElementById('tos-submit').disabled = false;
				$( document ).ready(function() {
					$('#tos-submit').addClass('clicked-tos');
				});
			} else {
				document.getElementById('tos-submit').disabled = true;
				$( document ).ready(function() {
					$('#tos-submit').removeClass('clicked-tos');
				});
			}
		}
